<!DOCTYPE html>
<html>
<head>
  <title>Paid</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>

<nav>
  <ul>
    <% if user_signed_in? %>
      <li><%= link_to 'Sign Out', '#' %></li>
    <% end %>
  </ul>
</nav>
<body>
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
  <%= yield %>
</body>
</html>
