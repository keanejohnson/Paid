<h1> PAID </h1>

<%= form_for @bill do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :amount %>
  <%= f.text_field :amount %>

  <%= f.label :party_size, "Party Size" %>
  <%= f.text_field :party_size %>

  <%= f.submit %>
<% end %>
